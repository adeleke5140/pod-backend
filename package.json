{
  "name": "backend",
  "version": "0.0.!",
  "main": "./dist/index.js",
  "description": "Express API starter using typescript for Nodejs.",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=6.13.0",
    "node": ">=14.0.0 <17.5.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engineStrict": true,
  "scripts": {
    "dev": "nodemon",
    "debug": "nodemon --exec \"node -r ts-node/register --inspect ./src/config/server/index.ts\"",
    "ts:watch": "tsc -w",
    "heroku:login": "heroku login",
    "heroku:create": "heroku create",
    "deploy:heroku": "git push heroku main",
    "test": "KAFKAJS_LOG_LEVEL=NOTHING NODE_ENV=test jest --runInBand --forceExit",
    "coverage": "NODE_ENV=test jest --coverage --runInBand --forceExit",
    "test:watch": "NODE_ENV=test jest --watch --runInBand --forceExit",
    "command": "npx ts-node ./src/commands",
    "build": "tsc --skipLibCheck",
    "lint": "yarn lint:ts",
    "format": "eslint src/**/*.ts --fix",
    "gen:typings": "./.scripts/build-typing.sh",
    "lint:ts": "eslint \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "api-flow": "ts-node __api_flows__/api-flow.ts",
    "start": "node ./dist/src/config/server/index.js",
    "lint:swagger": "spectral -r ./.spectral.json lint swagger/*.json",
    "prettier": "prettier --write \"src/**/*.{ts,json}\" \"__tests__/**/*.{ts,json}\" \"__api_flows__/**/*.ts\"",
    "commit": "git-cz",
    "lint:messages": "ts-node .scripts/message.ts",
    "lint:file-or-directory-name": "ts-node .scripts/lint-file-or-directory-name.ts"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@types/cookie-session": "^2.0.43",
    "@types/uuid": "^8.3.0",
    "axios": "0.21.1",
    "bcryptjs": "^2.4.3",
    "big-number": "^2.0.0",
    "big.js": "^6.1.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "dotenv": "8.2.0",
    "emittery": "0.8.1",
    "ethers": "^5.5.3",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "http-status-codes": "2.1.4",
    "ioredis": "4.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "5.11.15",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "qs": "^6.10.1",
    "rss-parser": "^3.12.0",
    "tracer": "1.1.4",
    "ts-nats": "1.2.15",
    "ts-node": "9.1.1",
    "twit": "^2.2.11",
    "uuid": "^8.3.2",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@stoplight/spectral": "5.8.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/big.js": "^6.1.0",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "8.2.0",
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/express": "4.17.11",
    "@types/ioredis": "4.19.3",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "1.9.2",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.25",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.1",
    "@types/nodemon": "1.19.0",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/prettier": "2.2.0",
    "@types/qs": "^6.9.6",
    "@types/recursive-readdir": "2.2.0",
    "@types/rimraf": "3.0.0",
    "@types/supertest": "2.0.10",
    "@types/twit": "^2.2.28",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "dtsgenerator": "3.7.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-unused-imports": "1.0.3",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "json-schema-to-typescript": "10.1.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "recursive-readdir": "2.2.2",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.0",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": "prettier --write",
    "__tests__/**/*.{ts}": "prettier --write",
    "__api_flows__/**/*.ts": "prettier --write",
    "swagger/**/*.json": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
